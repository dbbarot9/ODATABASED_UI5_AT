<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:core='sap.ui.core' controllerName="sapOdata.controller.View2">
<Page title="View-2" navButtonPress="callView" showNavButton="true" >
<content>
<ObjectHeader title="{PRODUCT_ID}" 
intro="{DESCRIPTION}" 
icon="{PRODUCT_PIC_URL}"
number="{PRICE}"
numberUnit="{CURRENCY_CODE}"
></ObjectHeader>
<IconTabBar >
<items>
<IconTabFilter text="First" iconColor="Positive" icon="sap-icon://car-rental"
>
<core:Fragment type='XML' fragmentName='sapOdata.fragments.moreinfo'/>
</IconTabFilter>
<IconTabSeparator ></IconTabSeparator>
<IconTabFilter text="Second" icon="sap-icon://inventory" iconColor="Negative"
>
<core:Fragment type='XML' fragmentName='sapOdata.fragments.moreinfo'/>
</IconTabFilter>
<IconTabSeparator ></IconTabSeparator>
<IconTabFilter text="Third" icon="sap-icon://subway-train" iconColor="Neutral"
>
<core:Fragment type='XML' fragmentName='sapOdata.fragments.moreinfo'/>
</IconTabFilter>
</items>
</IconTabBar>
</content>
</Page>
</mvc:View>